@model List<Brand>
<div>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Nombre</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var brand in Model)
            {
                <tr>
                    <td>@brand.ID</td>
                    <td>@brand.Name</td>
                </tr>
            }
        </tbody>
    </table>
</div>